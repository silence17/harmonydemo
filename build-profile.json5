{
  "app": {
    "signingConfigs": [
    ],
    "compileSdkVersion": 9,
    "compatibleSdkVersion": 9,
    //定义构建的产品品类，如通用默认版、付费版、免费版等
    "products": [
      {
        //定义产品的名称，由开发者自定义
        "name": "default",
        //指定当前产品品类对应的签名信息，签名信息需要在signingConfigs中进行定义
        "signingConfig": "default",
      }
    ],
  },
  "modules": [
    {
      "name": "entry",
      "srcPath": "./entry",
      "targets": [
        {
          "name": "default",
          "applyToProducts": [
            "default"
          ]
        }
      ]
    },
    {
      "name": "common_lib",
      "srcPath": "./common_lib"
    },
    {
      "name": "library",
      "srcPath": "./library"
    }
  ]
}